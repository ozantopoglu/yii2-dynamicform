{
    "name": "ozantopoglu/yii2-dynamicform",
    "description": "It is widget to yii2 framework to clone form elements in a nested manner, maintaining accessibility.",
    "keywords": [
        "wbraganca",
        "ozantopoglu",
        "yii2",
        "extension",
        "widget",
        "yii2-dynamicform",
        "copy DOM element",
        "yii2 dynamic form"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/ozantopoglu/yii2-dynamicform/issues",
        "wiki": "https://github.com/ozantopoglu/yii2-dynamicform/wiki/",
        "source": "https://github.com/ozantopoglu/yii2-dynamicform"
    },
    "authors": [
        {
            "name": "Ozan Topoglu",
            "email": "ozantopoglu@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.54",
        "symfony/css-selector": "*",
        "symfony/dom-crawler": "*"
    },
    "autoload": {
        "psr-4": {
            "ozantopoglu\\dynamicform\\": "src"
        }
    }
}
